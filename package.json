{
  "name": "zumly",
  "version": "0.2.0",
  "main": "dist/zumly.min.mjs",
  "module": "dist/zumly.mjs",
  "browser": "dist/zumly.umd.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zumly"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "concurrently": "4.1.2",
    "jest": "24.9.0",
    "rollup": "1.25.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-babel-minify": "^9.1.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "serve": "11.2.0",
    "standard": "^14.3.1"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "watch": "concurrently 'yarn build:watch' 'yarn serve -l tcp://127.0.0.1'",
    "test": "jest && yarn build",
    "lint": "standard 'src/*.js'"
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    },
    "automergeType": "branch"
  },
  "files": [
    "dist"
  ]
}
